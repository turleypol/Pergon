Use uo;
Use os;

Include "include/msgs";

Program TextCMD_UnloadAll(who)
  Var res:=Unload_Scripts();
  If (res==error)
    SendSysMessagePergon(who, "Fehler beim Entfernen der Scripte: "+res.errortext);
  ElseIf (res<0)
    SendSysMessagePergon(who, "Es war kein Script geladen!");
  ElseIf (res==1)
    SendSysMessagePergon(who, "1 Script-Referenz aus dem Cache entfernt.");
  Else
    SendSysMessagePergon(who, res+" Script-Referenzen aus dem Cache entfernt.");
  EndIf
EndProgram