// Prueft, ob das uebergebene Objekt ein Baum(teil) ist
Function IsTree(theobj) // {{{
  If (theobj.isa(POLCLASS_ITEM))
    theobj := theobj.objtype;
  EndIf

  If (theobj In {
      0x0c95, 0x0c96, 0x0ca8, 0x0caa, 0x0cab, 0x0d37, 0x0d38, 0x224a,
      0x224b, 0x224c, 0x224d, 0x2471, 0x247a, 0x247d, 0x247e, 0x26ed,
      0x26f3
  })
    return 1;
  ElseIf ((theobj >= 0xcca) && (theobj <= 0xce8))
    return 1;
  ElseIf ((theobj >= 0xcf8) && (theobj <= 0xd03))
    return 1;
  ElseIf ((theobj >= 0xd41) && (theobj <= 0xd90))
    return 1;
  ElseIf ((theobj >= 0xd94) && (theobj <= 0xdab))
    return 1;
  EndIf

  return 0;
EndFunction // }}}

// Prueft, ob Mobile oder ObjType ein Elf ist
Function IsElf(obj) // {{{
  If (obj.isa(POLCLASS_MOBILE))
    obj := obj.objtype;
  EndIf

  If (obj in (array{UOBJ_ELF_MALE, UOBJ_ELF_FEMALE}))
    return 1;
  EndIf

  return 0;
EndFunction // }}}

// Prueft, ob Mobile oder ObjType ein Mensch ist
Function IsHuman(obj) // {{{
  If (obj.isa(POLCLASS_MOBILE))
    obj := obj.objtype;
  EndIf

  If (obj in (array{UOBJ_HUMAN_MALE, UOBJ_HUMAN_FEMALE}))
    return 1;
  EndIf

  return 0;
EndFunction // }}}

// Kombination aus beiden Varianten
Function IsElfOrHuman(obj) // {{{
  If (obj.isa(POLCLASS_MOBILE))
    obj := obj.objtype;
  EndIf

  // wir haben mehr Menschen als Elfen
  If (IsHuman(obj))
    return 1;
  EndIf

  return IsElf(obj);
EndFunction // }}}

